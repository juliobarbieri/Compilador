
int main()
{
	int linhas = 2;
	int colunas = 2;
	
	int temp;
	int matriz[2][2];
	
	for(int i = 0; i < linhas; i++) {
		for(int j = 0; j < colunas; j++) {
			read(temp);
			matriz[i][j] = temp;
		}
	}
	
	write("Matriz A: ");
	for(int k = 0; k < linhas; k++) {
		for(int t = 0; t < colunas; t++) {
			write(matriz[k][t]);
		}
		write("Fim linha");
	}

}
